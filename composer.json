{
    "name": "jstalinko/tokoshani-vipreseller",
    "description": "TokoShani intregation with vip-reseller.co.id provider",
    "keywords": [
        "jstalinko",
        "tokoshani-vipreseller"
    ],
    "homepage": "https://github.com/jstalinko/tokoshani-vipreseller",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "alinko",
            "email": "alinkokomansuby@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Jstalinko\\TokoshaniVipreseller\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jstalinko\\TokoshaniVipreseller\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jstalinko\\TokoshaniVipreseller\\TokoshaniVipresellerServiceProvider"
            ],
            "aliases": {
                "TokoshaniVipreseller": "Jstalinko\\TokoshaniVipreseller\\TokoshaniVipresellerFacade"
            }
        }
    }
}
